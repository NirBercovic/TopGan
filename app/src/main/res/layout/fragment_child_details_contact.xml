<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ChildDetailsScreen.ui.childdetails.tabs.ChildDetailsContactFragment">

    <TextView
        android:id="@+id/tv_cd_parent1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/tv_cd_parentPhone1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintTop_toTopOf="@+id/tv_cd_parent1"
        app:layout_constraintStart_toEndOf="@+id/tv_cd_parent1"/>

    <TextView
        android:id="@+id/tv_cd_parent2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_cd_parent1"
        app:layout_constraintStart_toStartOf="@+id/tv_cd_parent1"/>

    <TextView
        android:id="@+id/tv_cd_parentPhone2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintTop_toTopOf="@+id/tv_cd_parent2"
        app:layout_constraintStart_toEndOf="@+id/tv_cd_parent2"/>

    <TextView
        android:id="@+id/tv_cd_contact1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_cd_parent2"
        app:layout_constraintStart_toStartOf="@+id/tv_cd_parent2" />

    <TextView
        android:id="@+id/tv_cd_relation1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintTop_toTopOf="@+id/tv_cd_contact1"
        app:layout_constraintStart_toEndOf="@+id/tv_cd_contact1"/>

    <TextView
        android:id="@+id/tv_cd_contactPhone1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintTop_toTopOf="@+id/tv_cd_relation1"
        app:layout_constraintStart_toEndOf="@+id/tv_cd_relation1"/>


    <TextView
        android:id="@+id/tv_cd_contact2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_cd_contact1"
        app:layout_constraintStart_toStartOf="@+id/tv_cd_contact1"/>

    <TextView
        android:id="@+id/tv_cd_relation2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintTop_toTopOf="@+id/tv_cd_contact2"
        app:layout_constraintStart_toEndOf="@+id/tv_cd_contact2"/>

    <TextView
        android:id="@+id/tv_cd_contactPhone2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintTop_toTopOf="@+id/tv_cd_relation2"
        app:layout_constraintStart_toEndOf="@+id/tv_cd_relation2"/>


    

</android.support.constraint.ConstraintLayout>